#!nsh
#
# @name X340
#
# @type Quadrotor x
#

sh /etc/init.d/4001_quad_x


if [ $AUTOCNF == yes ]
then

	# Battery calibration and scaling
	param set MC_BAT_SCALE_EN 1
	param set BAT_R_INTERNAL 0.016
	param set BAT_N_CELLS 3
	param set BAT_CAPACITY 2700
	param set BAT_V_EMPTY 3.4
	param set BAT_V_CHARGED 4.08


fi
