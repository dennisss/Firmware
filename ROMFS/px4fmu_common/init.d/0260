#!nsh
#
# @name X260
#
# @type Quadrotor x
#

sh /etc/init.d/4001_quad_x


if [ $AUTOCNF == yes ]
then

	param set SENS_BOARD_Z_OFF 25

	# Battery calibration and scaling
	param set MC_BAT_SCALE_EN 1
	param set BAT_R_INTERNAL 0.03
	param set BAT_N_CELLS 2
	param set BAT_CAPACITY 1500
	param set BAT_V_EMPTY 3.4
	param set BAT_V_CHARGED 4.02


	param set MC_PITCH_P 4.5
	param set MC_PITCHRATE_P 0.09
	param set MC_PITCHRATE_I 0.025
	param set MC_PITCHRATE_D 0.004

	param set MC_ROLL_P 4.5
	param set MC_ROLLRATE_P 0.09
	param set MC_ROLLRATE_I 0.025
	param set MC_ROLLRATE_D 0.004

	param set THR_MDL_FAC 0.65

fi
